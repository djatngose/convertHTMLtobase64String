@section scripts
{
    <script>
        $(function () {
            $.get("api/values/1", function (data) {
         
                var dec = window.atob(data);
                document.getElementById("result1").innerHTML = decodeURIComponent(escape(dec));
               // $(".result").attr("src", "data:image/png;base64," + data);
            });
        });
    </script>
 
}